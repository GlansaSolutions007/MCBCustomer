      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB1e_nM-v-G5EYZSrXjElyHo61I4qb5rNc"
        }
      }
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6850b1f3-7aec-4d07-8b8b-1802366d658a"
      }
